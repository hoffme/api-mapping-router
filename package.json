{
  "name": "api-mapping-router",
  "version": "2.0.0",
  "author": "Juan Ignacio Coronel",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hoffme/api-mapping-router.git"
  },
  "bugs": {
    "url": "https://github.com/hoffme/api-mapping-router/issues"
  },
  "files": [
    "lib/**/*"
  ],
  "exports": {
    "./map": "./lib/map.js",
    "./server": "./lib/server.js",
    "./client": "./lib/client.js"
  },
  "typesVersions": {
    "*": {
      "map": ["lib/map.d.ts"],
      "server": ["lib/server.d.ts"],
      "client": ["lib/client.d.ts"]
    }
  },
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "lint": "npx eslint src",
    "lint:fix": "npx lint -- --fix",
    "format": "npx prettier src --check",
    "format:fix": "npx prettier -- --write",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test && yarn lint",
    "preversion": "yarn lint",
    "version": "yarn format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.3",
    "prettier": "^2.8.8",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
